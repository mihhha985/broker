{
  "chart":{
    "layout": {
      "width": "100%", 
      "textColor": "#03a9f4", 
      "background": {"type":"solid", "color": "#fff"},
      "fontFamily": "'Roboto', sans-serif"
    },
    "grid": { 
      "vertLines": { "color": "blue"},
      "horzLines": { "color": "blue" }
    },
    "rightPriceScale": { "borderColor": "blue" },
    "timeScale": { 
      "borderColor": "blue", 
      "barSpacing": 10,
      "timeVisible": true,
      "ticksVisible": true,
      "secondsVisible":false 
    },
    "crosshair": {
      "mode": 0,
      "vertLine": {
        "color":"#03a9f4",
        "labelBackgroundColor":"#03a9f4"
      },
      "horzLine": {
        "color": "#03a9f4",
        "labelBackgroundColor":"#03a9f4"
      }
    }
  },
  "candlestickSeries":{
    "upColor": "#4caf50", 
    "downColor": "#ef5350",
    "borderVisible": false, 
    "wickUpColor": "#ef5350", 
    "wickDownColor": "#7b1fa2"
  }
}